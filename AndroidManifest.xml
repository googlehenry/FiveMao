<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ex.fivemao"
    android:versionCode="5"
    android:versionName="3.6稳定版" >

    <uses-sdk
        android:minSdkVersion="20"
        android:targetSdkVersion="20" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
	<uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION"/>
<!--     <uses-permission android:name="android.permission.RESTART_PACKAGES"/> -->
<!-- 	<uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>  -->
         
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    
	<uses-permission android:name="android.permission.RECORD_AUDIO"/>
<!--      <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS"/> -->
     
     
     
    <application
        android:name="FiveMaoApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="com.ex.fivemao.fileProvider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/update_files" />
        </provider>
          
        <service 
            android:name=".audioservice.MusicService">  
        </service>  
        
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity  
      		android:name=".VideoActivity"  
      		android:parentActivityName=".MainActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="视频列表"
      		>  
		</activity> 
        <activity  
      		android:name=".VideoDescActivity"  
      		android:parentActivityName=".VideoActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="视频介绍" >  
		</activity> 
        <activity  
      		android:name=".VideoPlayerActivity" 
      		android:parentActivityName=".VideoDescActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="视频播放" >  
		</activity> 
        <activity  
      		android:name=".VideoDuoPlayerActivity" 
      		android:parentActivityName=".VideoDescActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="视频播放" >  
		</activity> 
<!--         <activity   -->
<!--       		android:name=".TestingMediaPlayerActivity"  -->
<!--       		android:parentActivityName=".VideoDescActivity" -->
<!--       		android:configChanges="orientation|keyboardHidden|screenSize" -->
<!--       		android:label="TestingPlayer" >   -->
<!-- 		</activity>  -->
        <activity  
      		android:name=".TestAudioVisualizerActivity"  
      		android:parentActivityName=".MainActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="音频频谱" >  
		</activity> 
        <activity  
      		android:name=".VideoBuyActivity"  
      		android:parentActivityName=".VideoPlayerActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="视频购买" >  
		</activity> 
        <activity  
      		android:name=".MeActivity"  
      		android:parentActivityName=".MainActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="个人信息" >  
		</activity> 
        <activity  
      		android:name=".MeLoginActivity"  
      		android:parentActivityName=".MeActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="登录/注册界面" >  
		</activity> 
        <activity  
      		android:name=".MeBoughtMembershipActivity"  
      		android:parentActivityName=".MeActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="购买会员记录" >  
		</activity> 
        <activity  
      		android:name=".MeBoughtVideosActivity"  
      		android:parentActivityName=".MeActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="已购电影列表" >  
		</activity> 
        <activity  
      		android:name=".MeVisitedVideosActivity"  
      		android:parentActivityName=".MeActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="播放历史列表" >  
		</activity> 
        <activity  
      		android:name=".MeRecDownsActivity"  
      		android:parentActivityName=".MeActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="推荐佣金" >  
		</activity> 
        <activity  
      		android:name=".VideoBuyMembershipActivity"  
      		android:parentActivityName=".MeActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="购买会员" >  
		</activity> 
        <activity  
      		android:name=".SearchActivity"  
      		android:parentActivityName=".MainActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="视频搜索" >  
		</activity> 
        <activity  
      		android:name=".BoardActivity"  
      		android:parentActivityName=".MainActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="反馈留言" >
		</activity> 
        <activity  
      		android:name=".HotHotActivity"  
      		android:parentActivityName=".MainActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="热播影片" >
		</activity> 
        <activity  
      		android:name=".BoardMsgDescActivity"  
      		android:parentActivityName=".BoardActivity"
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="留言详细" >
		</activity> 
		<activity  
      		android:name=".MenuSettingsActivity"  
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="系统设置" >  
		</activity> 
		<activity  
      		android:name=".ReconToFriendsActivity"  
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="推荐APP给朋友" >  
		</activity> 
		<activity  
      		android:name=".HelpGuideActivity"  
      		android:configChanges="orientation|keyboardHidden|screenSize"
      		android:label="购买指南" >  
		</activity> 
    </application>
    

</manifest>
